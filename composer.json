{
  "name": "php-woox/webman-validate",
  "description": "validate for webman plugin",
  "keywords": ["validate", "webman", "plugin"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "codeloving",
      "email": "codeloving@163.com"
    }
  ],
  "require": {
    "php": ">=8.2",
    "ext-mbstring": "*",
    "ext-ctype": "*"
  },
  "autoload": {
    "psr-4": {
      "Woox\\WebmanValidate\\": "src"
    },
    "files": [
      "src/helper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Woox\\Webman\\Validate\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "workerman/webman": "^1.5"
  }
}